{
  "editor.formatOnSave": true,
  "eslint.run": "onSave",
  "eslint.validate": ["typescript"],
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true
  },
  "git.ignoreLimitWarning": true,
  "github.gitAuthentication": true,
  "search.exclude": {
    "**/dist": true,
    "**/lib": true,
    "**/node_modules": true
  },
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.quoteStyle": "single"
}
